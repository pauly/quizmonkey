{
  "name": "quizmonkey",
  "version": "1.0.0",
  "description": "Quiz",
  "main": "build/index.js",
  "scripts": {
    "coverage": "nyc mocha && nyc check-coverage",
    "prebuild": "npm run data",
    "data": "./scripts/yml2json.js",
    "build": "NODE_ENV=production webpack",
    "lint": "standard --fix",
    "start": "npm run build && open docs/index.html",
    "test": "npm run lint && npm run coverage",
    "prewatch": "npm run data",
    "watch": "open http://localhost:8097/webpack-dev-server/ && webpack-dev-server --inline --hot --port 8097"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pauly/quizmonkey.git"
  },
  "keywords": [
    "quiz",
    "react",
    "italian",
    "german"
  ],
  "author": "Paul Clarke <pauly@clarkeology.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pauly/quizmonkey/issues"
  },
  "homepage": "https://github.com/pauly/quizmonkey#readme",
  "standard": {
    "global": [
      "sinon",
      "sandbox",
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "context",
      "describe",
      "expect",
      "it"
    ]
  },
  "nyc": {
    "all": true,
    "extension": [
      ".js"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "statements": 100,
    "lines": 100,
    "branches": 100,
    "functions": 100,
    "exclude": [
      "coverage",
      "docs",
      "scripts",
      "webpack.config.js"
    ]
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.6.5",
    "brfs": "^1.4.3",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "dirty-chai": "^1.2.2",
    "extract-text-webpack-plugin": "3.0.0",
    "fs": "0.0.2",
    "html-webpack-inline-source-plugin": "0.0.9",
    "html-webpack-plugin": "2.30.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "nyc": "11.2.1",
    "serve": "10.1.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "standard": "10.0.3",
    "transform-loader": "^0.2.3",
    "url-loader": "0.5.9",
    "webpack": "2.7.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "4.1.0",
    "yamljs": "^0.2.6"
  },
  "dependencies": {
    "vue": "2.6.6",
    "vue-loader": "14.2.2",
    "vue-router": "3.0.2",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.6",
    "webpack-pwa-manifest": "4.0.0",
    "yaml-loader": "0.5.0"
  }
}
